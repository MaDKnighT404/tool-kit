{
  "name": "toolkit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "coverage": "vitest run --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:report": "npx nyc report && npx nyc report --reporter=text-summary",
    "cypress:console": "start-server-and-test dev http://localhost:5173 cy:run",
    "cypress:browser": "start-server-and-test dev http://localhost:5173 cy:open",
    "coverage:e2e": "npm run cypress:console && npm run cy:report"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.11.2",
    "sass": "^1.62.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/node": "^20.2.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "cypress": "^12.13.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  }
}
